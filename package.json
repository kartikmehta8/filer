{
  "name": "filer",
  "version": "1.0.0",
  "description": "A Docusaurus Alternative based on React & Parcel",
  "scripts": {
    "generate-routes": "node generateRoutes.js",
    "start": "npm run generate-routes && parcel public/index.html",
    "build": "npm run generate-routes && parcel build public/index.html",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,md}\""
  },
  "author": "kartikmehta8",
  "license": "MIT",
  "dependencies": {
    "markdown-to-jsx": "^7.4.7",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.25.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "parcel": "^2.12.0",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.5"
  },
  "targets": {
    "default": {
      "source": "src/index.html",
      "distDir": "dist",
      "publicUrl": "/"
    }
  },
  "staticFiles": {
    "staticPath": "src/docs"
  }
}
